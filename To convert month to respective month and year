#include <stdio.h>
int main(){
int m;
char a[99]="JANFEBMARAPRMAYJUNJULAUGSEPOCTNOVDEC";
scanf("%4s%2d",a+50,&m);//4s takes 4 characters in a string and 2d takes two numbers 
a[m*3]=0;//makes the remaining characters to zero

printf("%s\n%s",a+(m-1)*3,a+52); //to print month and year
    
}


INPUT: 201608
OUTPUT :AUG16
